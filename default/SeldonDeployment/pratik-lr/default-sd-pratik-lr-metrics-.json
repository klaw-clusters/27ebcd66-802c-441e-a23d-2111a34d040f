{
    "kind": "Service",
    "apiVersion": "serving.knative.dev/v1",
    "metadata": {
        "name": "default-sd-pratik-lr-metrics-",
        "namespace": "default",
        "creationTimestamp": null,
        "labels": {
            "seldon.io/deployment-kind": "SeldonDeployment",
            "seldon.io/deployment-name": "pratik-lr",
            "seldon.io/deployment-namespace": "default",
            "seldon.io/detector-name": "",
            "seldon.io/detector-type": "metrics"
        }
    },
    "spec": {
        "template": {
            "metadata": {
                "creationTimestamp": null,
                "labels": {
                    "fluentd": "true",
                    "seldon-deployment-id": "pratik-lr",
                    "seldon.io/metrics": "true"
                },
                "annotations": {
                    "autoscaling.knative.dev/maxScale": "1",
                    "autoscaling.knative.dev/minScale": "1",
                    "prometheus.io/path": "/v1/metrics",
                    "prometheus.io/port": "8080",
                    "prometheus.io/scrape": "true"
                }
            },
            "spec": {
                "containers": [
                    {
                        "name": "metrics-detector",
                        "image": "seldonio/alibi-detect-server:1.11.2",
                        "args": [
                            "--http_port",
                            "8080",
                            "--protocol",
                            "tensorflow.http",
                            "--storage_uri",
                            "gs://tom-seldon-examples/retail-workshop/models/pratik/lr/metrics",
                            "--reply_url",
                            "http://seldon-request-logger.seldon-logs",
                            "--event_type",
                            "io.seldon.serving.feedback.metrics",
                            "--event_source",
                            "io.seldon.serving.pratik-lr-",
                            "MetricsServer",
                            "--drift_batch_size",
                            "1000"
                        ],
                        "ports": [
                            {
                                "containerPort": 8080,
                                "protocol": "TCP"
                            }
                        ],
                        "env": [
                            {
                                "name": "SELDON_DEPLOYMENT_ID",
                                "value": "pratik-lr"
                            },
                            {
                                "name": "SELDON_DEPLOYMENT_NAMESPACE",
                                "value": "default"
                            }
                        ],
                        "resources": {},
                        "securityContext": {
                            "runAsUser": 8888
                        }
                    }
                ]
            }
        }
    },
    "status": {}
}